package com.venkat;

   class human {
	String name = "This is human";
}
 class female extends human{
	String name = "She is female";
	public void fun() {
		System.out.println(super.name);
	}
}
 class venkat extends female {
	 String name = "Name is abisha";
 }
public class Poly {
	public static void main(String[] args) {
		female s = new female();
		venkat v = new venkat();
		
		
		s.fun();
		System.out.println(s.name);
		System.out.println(v.name);
		
	}
}

