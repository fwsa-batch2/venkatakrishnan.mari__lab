package com.venkat;

  abstract class mobile{
	  void display();
	  void charger();
	  
	
  } 
  class redmi extends mobile{
	  public void display() {
		  System.out.println("Display is super");
	  }
	  public void charger() {
		  System.out.println("Charger is powerful");
	  }
	  
  }
public class abstracts {

	public static void main(String[] args) {
		redmi v = new redmi();
		v.display();
		v.charger();

	}

}
