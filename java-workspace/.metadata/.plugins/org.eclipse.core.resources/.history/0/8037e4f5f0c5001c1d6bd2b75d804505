package com.bank;

public class Bank {
           int a = 2004;
          public int b = 2003;
          private int c = 2005;
          protected int d = 2006;
	public static void main(String[] args) {
		 Bank obj = new Bank();
		 System.out.println(obj.a);
		 System.out.println(obj.b);
		 System.out.println(obj.c);
		 System.out.println(obj.d);
		 
		 
//		 Bank1 obj1 = new Bank1();
